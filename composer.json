{
    "name": "toin0u/concise",
    "description": "Concise your urls via extern providers",
    "license": "MIT",
    "keywords": ["url", "shortener", "shorten", "api"],
    "homepage": "https://github.com/toin0u/concise",
    "authors": [
        {
            "name": "Antoine Corcy",
            "email": "contact@sbin.dk",
            "homepage": "http://sbin.dk",
            "role": "Developer"
        },
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "php-http/httplug": "^1.0",
        "php-http/message-factory": "^1.0"
    },
    "require-dev": {
        "tedivm/stash": "dev-master",
        "phpspec/phpspec": "~2.1",
        "henrikbjorn/phpspec-code-coverage" : "~1.0"
    },
    "suggest": {
        "php-http/client-implementation": "An HTTP Client implementation",
        "php-http/message": "Provides Message Factory implementations",
        "tedivm/stash": "Used for caching URLs"
    },
    "autoload": {
        "psr-4": {
            "Concise\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
